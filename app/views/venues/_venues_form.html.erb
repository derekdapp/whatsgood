<%= simple_form_for @venue, html: { role: 'form'} do |f| %>
  <%= f.input :name, placeholder: 'Name', label: false %>
  <%= f.input :description, placeholder: 'Description', label: false %>
  <%= f.input :venue_type, placeholder: 'Venue Type', label: false  %>
  <!-- #### Adding Venue Genre Below ##### -->
  <%= f.input :genre, placeholder: 'Genre', label: false  %>
  <%= f.input :address, placeholder: 'Address', label: false  %>
  <%= f.input :phone, placeholder: 'Phone', label: false %>
  <%= f.input :email, placeholder: 'Email', label: false %>
  <%= f.input :website, placeholder: 'website', label: false %>
  <%= f.button :submit %>
<% end %>
